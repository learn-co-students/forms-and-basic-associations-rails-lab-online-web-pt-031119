<h1>Create a Song</h1>

<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name%>
  <%= f.text_field :artist_name%>
  <%= f.label :genre_id%>
  <%= f.collection_select :genre_id, Genre.all, :name, :name%>
  <%= f.label :notes_1%>
  <%= f.text_field :notes_1%>
  
  <br>

  <%= f.submit %>
<% end %>


